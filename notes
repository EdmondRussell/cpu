export CPPFLAGS="-I/usr/include/openssl"
export LDFLAGS="-L/usr/lib/x86_64-linux-gnu"
export OPENSSL_LIBS="-lssl -lcrypto"
make distclean  # full clean
./autogen.sh
./configure CFLAGS="-O3 -Wall -march=native -DNO_ASM" --with-curl
